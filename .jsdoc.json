{
  "source": {
    "include": ["./src/server/"],
    "includePattern": "\\.(js)$",
    "exclude": ["node_modules/", "tests/"]
  },
  "opts": {
    "destination": "./docs/api/",
    "recurse": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "markdown": {
    "parser": "gfm"
  }
}